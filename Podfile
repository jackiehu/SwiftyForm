  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  inhibit_all_warnings!
  # Pods for SwiftyForm

  pod 'SwiftBrick'
  pod 'SwiftMediator'
  pod 'SwiftShow'
  pod 'Swift_Log'
  pod 'Swift_Notification'
  pod 'SwiftyRefresh'

end
